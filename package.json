{
	"name": "urn-cli",
	"version": "0.0.0",
	"description": "Uranio Command Line Interface",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc -b",
		"init": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js init -fvw -r api -p [CL]\"",
		"transpose": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js transpose -wv -p [TRA]\"",
		"help": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js -h -p\"",
		"test": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js test -vbwf --noblank\"",
		"version": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js -v\"",
		"dev": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js dev\"",
		"ubuild": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js build\"",
		"server:dev": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js server:dev\"",
		"client:dev": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js client:dev\"",
		"alias": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js alias -wv\"",
		"hooks": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js hooks -wv\"",
		"run": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register ./dist/test.js\""
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@bitbucket.org/nbl7/urn-cli.git"
	},
	"keywords": [
		"uranio",
		"urn"
	],
	"author": "Andrea Reni <mail@nbl7.net>",
	"license": "ISC",
	"homepage": "https://bitbucket.org/nbl7/urn-cli#readme",
	"devDependencies": {
		"@types/chokidar": "^2.1.3",
		"@types/inquirer": "^7.3.1",
		"@types/minimist": "^1.2.1",
		"@types/prettier": "^2.2.2",
		"urn-dev": "git+ssh://git@bitbucket.org/nbl7/urn-dev.git"
	},
	"dependencies": {
		"chalk": "^4.1.0",
		"chokidar": "^3.5.2",
		"dateformat": "^4.5.1",
		"inquirer": "^8.0.0",
		"minimist": "^1.2.5",
		"ora": "^5.3.0",
		"prettier": "^2.2.1",
		"ts-morph": "^10.0.1",
		"urn-lib": "git+ssh://git@bitbucket.org/nbl7/urn-lib.git"
	},
	"bin": {
		"uranio": "dist/sh.js"
	}
}
