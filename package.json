{
	"name": "uranio-cli",
	"version": "0.0.27",
	"description": "Uranio command line interface",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc -b",
		"init": "npx tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js init -fvw -r api -x [CL]\"",
		"transpose": "npx tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js transpose -w\"",
		"help": "npx tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js -h -x\"",
		"test": "npx tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js test -vbwf --noblank\"",
		"version": "npx tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js -v\"",
		"dev": "npx tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js dev\"",
		"ubuild": "npx tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js build\"",
		"server:dev": "npx tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js server:dev\"",
		"client:dev": "npx tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js client:dev\"",
		"alias": "npx tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js alias -uv\"",
		"hooks": "npx tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js hooks -wv\"",
		"watch": "npx tsc-watch --onSuccess \"node -r source-map-support/register ./dist/test.js\""
	},
	"engines": {
		"node": ">=14"
	},
	"engineStrict" : true,
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/nbl7/uranio-cli.git"
	},
	"keywords": [
		"uranio",
		"urn"
	],
	"author": "Andrea Reni <mail@nbl7.net>",
	"license": "ISC",
	"homepage": "https://github.com/nbl7/uranio-cli#readme",
	"devDependencies": {
		"@types/chokidar": "^2.1.3",
		"@types/fs-extra": "^9.0.13",
		"@types/inquirer": "^7.3.1",
		"@types/minimist": "^1.2.1",
		"@types/prettier": "^2.2.2",
		"uranio-dev": "git+ssh://git@github.com/nbl7/uranio-dev.git"
	},
	"dependencies": {
		"chalk": "^4.1.0",
		"chokidar": "^3.5.2",
		"dateformat": "^4.5.1",
		"esbuild": "^0.14.10",
		"inquirer": "^8.0.0",
		"minimist": "^1.2.5",
		"nodemon": "^2.0.15",
		"ora": "^5.3.0",
		"prettier": "^2.2.1",
		"ts-morph": "^10.0.1",
		"urn-lib": "git+ssh://git@github.com/nbl7/uranio-lib.git"
	},
	"bin": {
		"urn": "dist/sh.js",
		"uranio": "dist/sh.js"
	}
}
