{
	"name": "urn-cli",
	"version": "0.0.0",
	"description": "Uranio Command Line Interface",
	"main": "dist/urn-cli.js",
	"types": "dist/urn-cli.d.ts",
	"scripts": {
		"build": "tsc -b",
		"start": "node -r source-map-support/register dist/urn-cli.js",
		"init": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register ./dist/urn-cli.js init\"",
		"help": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register ./dist/urn-cli.js -h\"",
		"version": "./node_modules/.bin/tsc-watch --onSuccess \"node -r source-map-support/register ./dist/urn-cli.js -v\""
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@bitbucket.org/nbl7/urn-cli.git"
	},
	"keywords": [
		"uranio",
		"urn"
	],
	"author": "Andrea Reni <mail@nbl7.net>",
	"license": "ISC",
	"homepage": "https://bitbucket.org/nbl7/urn-cli#readme",
	"devDependencies": {
		"@types/minimist": "^1.2.1",
		"urn-dev": "git+ssh://git@bitbucket.org/nbl7/urn-dev.git"
	},
	"dependencies": {
		"minimist": "^1.2.5"
	},
	"bin": {
		"uranio": "dist/urn-cli.js"
	}
}
