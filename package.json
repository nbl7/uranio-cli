{
	"name": "uranio-cli",
	"version": "0.0.62",
	"description": "Uranio command line interface",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"init": "yarn tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js init -fvw -r api -x [CL] --branch master\"",
		"build": "yarn tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js build -fvw -r api -x [CL] --branch master\"",
		"dev": "yarn tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js dev --branch master\"",
		"help": "yarn tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js -h -x --branch master\"",
		"ver": "yarn tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js -v --branch master\"",
		"watch": "yarn tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js -vu --branch master\"",
		"transpose": "yarn tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js transpose -vu \"",
		"generate:register": "yarn tsc-watch --onSuccess \"node -r source-map-support/register ./dist/sh.js generate:register -vu \""
	},
	"engines": {
		"node": ">=14"
	},
	"engineStrict": true,
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/nbl7/uranio-cli.git"
	},
	"keywords": [
		"uranio",
		"urn"
	],
	"author": "Andrea Reni <mail@nbl7.net>",
	"license": "ISC",
	"homepage": "https://github.com/nbl7/uranio-cli#readme",
	"devDependencies": {
		"@types/chokidar": "^2.1.3",
		"@types/forever-monitor": "^1.7.6",
		"@types/fs-extra": "^9.0.13",
		"@types/inquirer": "^7.3.1",
		"@types/is-docker": "^2.0.0",
		"@types/minimist": "^1.2.1",
		"@types/prettier": "^2.2.2",
		"uranio-dev": "git+ssh://git@github.com/nbl7/uranio-dev.git#master"
	},
	"dependencies": {
		"@nbl7/ora": "^1.0.3",
		"babylon": "^6.18.0",
		"chalk": "^4.1.0",
		"chokidar": "^3.5.2",
		"dateformat": "^4.5.1",
		"esbuild": "^0.14.10",
		"forever": "^4.0.3",
		"forever-monitor": "^3.0.3",
		"inquirer": "^8.0.0",
		"is-docker": "^2.0.0",
		"minimist": "^1.2.5",
		"nodemon": "^2.0.15",
		"prettier": "^2.2.1",
		"recast": "^0.20.5",
		"toml": "^3.0.0",
		"ts-morph": "^10.0.1",
		"urn-lib": "git+ssh://git@github.com/nbl7/uranio-lib.git#master"
	},
	"bin": {
		"urn": "dist/sh.js",
		"uranio": "dist/sh.js"
	}
}
